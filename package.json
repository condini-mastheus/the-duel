{
  "name": "the-duel",
  "version": "0.0.1",
  "description": "An open source pvp game. Built with Phaser 3 and Colyseus",
  "homepage": "https://github.com/condini-mastheus/the-duel#readme",
  "scripts": {
    "start": "webpack serve --config webpack/webpack.dev.js",
    "build": "rimraf dist && webpack --config webpack/webpack.prod.js",
    "bundle": "npm run build",
    "serve": "serve dist",
    "format": "prettier --check src/scripts/**",
    "format:write": "prettier --write src/scripts/**"
  },
  "keywords": [
    "Phaser",
    "Phaser 3",
    "html5 game",
    "The Duel",
    "pvp game"
  ],
  "author": {
    "name": "Matheus Condini",
    "url": "https://github.com/condini-mastheus"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/condini-mastheus/the-duel.git"
  },
  "engines": {
    "node": ">=12"
  },
  "license": "MIT",
  "dependencies": {
    "phaser": "^3.55.2"
  },
  "devDependencies": {
    "copy-webpack-plugin": "^10.1.0",
    "html-webpack-plugin": "^5.5.0",
    "javascript-obfuscator": "^3.0.0",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "serve": "^13.0.2",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.3",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.6.0",
    "webpack-merge": "^5.8.0",
    "webpack-obfuscator": "^3.5.0",
    "workbox-webpack-plugin": "^6.4.2"
  }
}
